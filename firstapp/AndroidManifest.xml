<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.training.firstapp"
          android:versionCode="1"
          android:versionName="1.0">

    <uses-sdk
            android:minSdkVersion="14"
            android:targetSdkVersion="19"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_action_user"
            android:theme="@style/CustomActionBarTheme"
            >
        <activity android:name="com.training.firstapp.MyActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.training.firstapp.DisplayMessageActivity"
                  android:label="@string/app_name"
                  android:parentActivityName=".MyActivity"
                >
        </activity>

    </application>
</manifest>
